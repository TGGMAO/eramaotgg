ENDIF


;-------------------------------------------------
;調教開始時（2回目以降、X1をキャラ番号に変更する） CFLAGなし 
;-------------------------------------------------
@KX1_KOJO2
;反発刻印Lv3
IF MARK:3 == 3 && FLAG:7 == 2
	DRAWLINE
	PRINTFORMW 
	RETURN 1

;屈服刻印Lv0
ELSEIF MARK:2 == 0 && FLAG:7 == 2
	DRAWLINE
	PRINTFORMW 
	RETURN 1

;屈服刻印Lv1
ELSEIF MARK:2 == 1 && FLAG:7 == 2
	DRAWLINE
	PRINTFORMW 
	RETURN 1

;屈服刻印Lv2
ELSEIF MARK:2 == 2 && FLAG:7 == 2
	DRAWLINE
	PRINTFORMW 
	RETURN 1

;屈服刻印Lv3＋愛/淫乱無し
ELSEIF MARK:2 == 3 && TALENT:TARGET:85 == 0 && TALENT:TARGET:76 == 0 && FLAG:7 == 2
	DRAWLINE
	PRINTFORMW 
	RETURN 1

;淫乱
ELSEIF TALENT:TARGET:76 == 1 && FLAG:7 == 2
	DRAWLINE
	;ランダムで口上が変化する（使わない場合はすべて同じにすればよい）
	IF RAND:3 == 0
		PRINTFORMW 
	ELSEIF RAND:2 == 0
		PRINTFORMW 
	ELSE
		PRINTFORMW 
	ENDIF
	RETURN 1

;愛
ELSEIF TALENT:TARGET:85 == 1 && FLAG:7 == 2
	DRAWLINE
	;ランダムで口上が変化する（使わない場合はすべて同じにすればよい）
	IF RAND:3 == 0
		PRINTFORMW 
	ELSEIF RAND:2 == 0
		PRINTFORMW 
	ELSE
		PRINTFORMW 
	ENDIF
	RETURN 1
ENDIF
RETURN 0

;-------------------------------------------------
;EVENTEND関係（X1をキャラ番号に置換） CFLAG 211〜220を使用
;調教終了時のセリフ
;-------------------------------------------------
@EVENTEND
SIF FLAG:7 <= 0
	RETURN 0
SIF TALENT:160+X1 != 1
	RETURN 0

;キャラ死亡時は口上をスキップ
SIF BASE:0 <= 0
	RETURN 0

;--------------------------------------------------
;調教終了時のセリフ
;--------------------------------------------------
;反発刻印Lv3+愛なし
IF MARK:3 == 3 && TALENT:TARGET:85 == 0
	DRAWLINE
	PRINTFORMW 
	RETURN 1

;屈服刻印Lv1以下+愛なし
ELSEIF MARK:2 <= 1 && TALENT:TARGET:85 == 0
	DRAWLINE
	PRINTFORMW 
	RETURN 1

;屈服刻印Lv2+愛なし
ELSEIF MARK:2 == 2 && TALENT:TARGET:85 == 0
	DRAWLINE
	PRINTFORMW 
	RETURN 1

;屈服刻印Lv3+愛なし
ELSEIF MARK:2 == 3 && TALENT:TARGET:85 == 0
	DRAWLINE
	PRINTFORMW 
	RETURN 1

;淫乱(体力500以上)
ELSEIF TALENT:TARGET:76 == 1 && BASE:0 >= 500
	DRAWLINE
	PRINTFORMW 
	RETURN 1
;淫乱(体力500未満)
ELSEIF TALENT:TARGET:76 == 1 && BASE:0 <= 500
	DRAWLINE
	PRINTFORMW 
	RETURN 1

;愛(体力500以上)
ELSEIF TALENT:TARGET:85 == 1 && BASE:0 >= 500
	DRAWLINE
	PRINTFORMW 
	RETURN 1
;愛(体力500未満)
ELSEIF TALENT:TARGET:85 == 1 && BASE:0 <= 500
	DRAWLINE
	PRINTFORMW 
	RETURN 1
ENDIF
RETURN 0


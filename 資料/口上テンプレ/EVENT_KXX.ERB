	;初めて
	IF CFLAG:301 == 0
		;屈服刻印Lv2以上
		IF MARK:2 >= 2
			PRINTFORMW 
		;それ以外
		ELSE
			PRINTFORMW 
		ENDIF
		CFLAG:301 = 1
		RETURN 0
	;二回目以降
	ELSE
		;淫乱
		IF TALENT:TARGET:76 == 1 && (CFLAG:301 <= 5 || FLAG:7 == 2)
			PRINTFORMW 
			CFLAG:301 = 6
		;愛
		ELSEIF TALENT:TARGET:85 == 1 && (CFLAG:301 <= 4 || FLAG:7 == 2)
			PRINTFORMW 
			CFLAG:301 = 5
		;屈服刻印Lv3
		ELSEIF MARK:2 == 3 && (CFLAG:301 <= 3 || FLAG:7 == 2)
			PRINTFORMW 
			CFLAG:301 = 4
		;屈服刻印Lv2
		ELSEIF MARK:2 == 2 && (CFLAG:301 <= 2 || FLAG:7 == 2)
			PRINTFORMW 
			CFLAG:301 = 3
		;それ以外
		ELSEIF MARK:2 <= 1 && (CFLAG:301 <= 1 || FLAG:7 == 2)
			PRINTFORMW 
			CFLAG:301 = 2
		ENDIF
		RETURN 0
	ENDIF

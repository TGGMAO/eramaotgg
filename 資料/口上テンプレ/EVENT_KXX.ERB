;--------------------------------------------------
;口上ファイルの存在判定（X2をキャラ番号に置換）
;--------------------------------------------------
@EVENTTRAIN
#PRI
FLAG:1X2 = 1
SIF FLAG:7 == 0
	FLAG:7 = 2

@EVENTEND
#LATER
FLAG:1X2 = 0

;--------------------------------------------------
;EVENTTRAIN関係（X1をキャラ番号に置換）
;調教開始時のセリフ CFLAG 201〜219を使用
;-------------------------------------------------
@EVENTTRAIN
SIF FLAG:7 <= 0
	RETURN 0
SIF TALENT:160+X1 != 1
	RETURN 0

;-------------------------------------------------
;初調教時 CFLAG:201
;-------------------------------------------------
IF CFLAG:201 == 0
	DRAWLINE
	PRINTFORMW 
	CFLAG:201 = 1
	RETURN 1
;-------------------------------------------------
;NTR再捕獲 CFLAG:650〜660
;-------------------------------------------------
ELSEIF CFLAG:201 >= 1 && CFLAG:650 == 1
	IF TALENT:85 || TALENT:76
		DRAWLINE
		PRINTFORMW 
		;NTRスイッチ解除
		CFLAG:650 = 0
	ELSE
		DRAWLINE
		PRINTFORMW 
		;NTRスイッチ解除
		CFLAG:650 = 0
	ENDIF
	RETURN 1
;-------------------------------------------------
;屈服刻印（各Lv一回のみ） CFLAG:201
;-------------------------------------------------
;屈服刻印Lv1
ELSEIF CFLAG:201 < 2 && MARK:2 == 1
	DRAWLINE
	PRINTFORMW 
	CFLAG:201 = 2
	RETURN 1

;屈服刻印Lv2
ELSEIF CFLAG:201 < 3 && MARK:2 == 2
	DRAWLINE
	PRINTFORMW 
	CFLAG:201 = 3
	RETURN 1

;屈服刻印Lv3
ELSEIF CFLAG:201 < 4 && MARK:2 == 3 && TALENT:TARGET:85 == 0
	DRAWLINE
	PRINTFORMW 
	CFLAG:201 = 4
	RETURN 1

;淫乱
ELSEIF CFLAG:201 < 5 && TALENT:TARGET:76 == 1 && TALENT:TARGET:85 == 0
	DRAWLINE
	PRINTFORMW 
	CFLAG:201 = 5
	RETURN 1

;愛
ELSEIF CFLAG:201 < 6 && TALENT:TARGET:85 == 1
	DRAWLINE
	PRINTFORMW 
	CFLAG:201 = 6
	RETURN 1

;助手の有無をチェック（いない場合は二回目以降へ飛ぶ）
ELSEIF ASSI < 0 
	CALL KX1_KOJO2


;eraIM@Sから導入しました(eramaou)

;-------------------------------------------------
;애널봉사
;奉仕系コマンド：調教対象が調教者の애널を口で刺激
;-------------------------------------------------
@COM37

PRINTL 애널봉사

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:욕망
IF ABL:11
	SIF S
		PRINT  + 
	A += ABL:11 * 1
	PRINTS ABLNAME:11
	PRINTV 'LV,ABL:11,'(,ABL:11 * 1,')
	S = 1
ENDIF
;ABL:봉사정신
IF ABL:16
	SIF S
		PRINT  + 
	A += ABL:16 * 3
	PRINTS ABLNAME:16
	PRINTV 'LV,ABL:16,'(,ABL:16 * 3,')
	S = 1
ENDIF

IF MARK:1
	SIF S
		PRINT  + 
	A += MARK:1 * 2
	PRINTS MARKNAME:1
	PRINTV 'LV,MARK:1,'(,MARK:1 * 2,')
	S = 1
ENDIF

IF MARK:2
	SIF S
		PRINT  + 
	A += MARK:2 * 3
	PRINTS MARKNAME:2
	PRINTV 'LV,MARK:2,'(,MARK:2 * 3,')
	S = 1
ENDIF

IF MARK:3
	SIF S
		PRINT  - 
	A -= MARK:3 * 5
	PRINTS MARKNAME:3
	PRINTV 'LV,MARK:3,'(,MARK:3 * 5,')
	S = 1
ENDIF

;반항적
IF TALENT:11
	PRINT  - 
	A -= 5
	PRINTS TALENTNAME:11
	PRINTV '(,5,')
	S = 1
ENDIF
;프라이드높음
IF TALENT:15
	PRINT  - 
	A -= 12
	PRINTS TALENTNAME:15
	PRINTV '(,12,')
	S = 1
ENDIF
;프라이드낮음
IF TALENT:17
	PRINT  + 
	A += 2
	PRINTS TALENTNAME:17
	PRINTV '(,2,')
	S = 1
ENDIF
;저항
IF TALENT:34
	PRINT  - 
	A -= 20
	PRINTS TALENTNAME:34
	PRINTV '(,20,')
	S = 1
ENDIF
;수줍음
IF TALENT:35
	PRINT  - 
	A -= 1
	PRINTS TALENTNAME:35
	PRINTV '(,1,')
	S = 1
ENDIF
;악취둔감
IF TALENT:61
	SIF S
		PRINT  + 
	A += 2
	PRINTS TALENTNAME:61
	PRINTV '(,2,')
	S = 1
ENDIF
;악취민감
IF TALENT:62
	PRINT  - 
	A -= 5
	PRINTS TALENTNAME:62
	PRINTV '(,5,')
	S = 1
ENDIF
;헌신적
IF TALENT:63
	SIF S
		PRINT  + 
	A += 4
	PRINTS TALENTNAME:63
	PRINTV '(,4,')
	S = 1
ENDIF
;불결무시
IF TALENT:64
	SIF S
		PRINT  + 
	A += 3
	PRINTS TALENTNAME:64
	PRINTV '(,3,')
	S = 1
ENDIF
;쾌감을부정
IF TALENT:71
	PRINT  - 
	A -= 1
	PRINTS TALENTNAME:71
	PRINTV '(,1,')
	S = 1
ENDIF
;도착적
IF TALENT:80
	SIF S
		PRINT  + 
	A += 3
	PRINTS TALENTNAME:80
	PRINTV '(,3,')
	S = 1
ENDIF
;새드
IF TALENT:PLAYER:83
	SIF S
		PRINT  + 
	A += 3
	PRINTS TALENTNAME:83
	PRINTV '(,3,')
	S = 1
ENDIF
;애정
IF TALENT:85 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:85
	PRINTV '(,5,')
	S = 1
ENDIF
;獣姦
IF TEQUIP:89 && TALENT:136 == 0
	SIF S
		PRINT  - 
	A -= 20
	PRINTS ITEMNAME:22
	PRINTV '(,20,')
	S = 1
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:4 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:4 & 4
	Y += 3
;애널の汚れ
SIF STAIN:PLAYER:4 & 8
	Y += 7
;母乳の汚れ
SIF STAIN:PLAYER:4 & 16
	Y += 1
;尿の汚れ
SIF STAIN:PLAYER:4 & 32
	Y += 3

;獣姦の場合は汚れ7で固定
SIF TEQUIP:89
	Y = 7

SIF TALENT:61
	Y /= 3
SIF TALENT:62
	Y *= 2

;더러움 있음
IF Y
	PRINT  - 
	A -= Y
	;악취둔감
	IF TALENT:61
		PRINT 더러움 있음、
		PRINTS TALENTNAME:61
	ELSEIF TALENT:62
		PRINT 더러움 있음、
		PRINTS TALENTNAME:62
	ELSE
		PRINT 더러움 있음
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF


;合計を表示(23以上で実行)
PRINT  = 
PRINTV A

V = 23
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 실행값
PRINTV V

WAIT

;実行できない
SIF A < V
	RETURN 0

;SAVESTR:22 = 애널奉仕강제
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;実行決定
;-------------------------------------------------
;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:1 += 10
LOSEBASE:1 += 100

SOURCE:13 = 3000
SOURCE:14 = 5000

;上のほうで計算した汚れデータ
SOURCE:8 = Y*80 + 50

;ABL:奉仕精神をみる
IF ABL:16 == 0
	SOURCE:4 = 420
	SOURCE:5 = 150
	TIMES SOURCE:8 , 4.00
ELSEIF ABL:16 == 1
	SOURCE:4 = 500
	SOURCE:5 = 300
	TIMES SOURCE:8 , 2.50
ELSEIF ABL:16 == 2
	SOURCE:4 = 580
	SOURCE:5 = 600
	TIMES SOURCE:8 , 1.50
ELSEIF ABL:16 == 3
	SOURCE:4 = 660
	SOURCE:5 = 900
	TIMES SOURCE:8 , 1.00
ELSEIF ABL:16 == 4
	SOURCE:4 = 740
	SOURCE:5 = 1500
	TIMES SOURCE:8 , 0.50
ELSE
	SOURCE:4 = 820
	SOURCE:5 = 2200
	TIMES SOURCE:8 , 0.10
ENDIF


;ABL:技巧をみる
IF ABL:12 == 0
	TIMES SOURCE:4 , 0.50
	TIMES SOURCE:5 , 0.50
ELSEIF ABL:12 == 1
	TIMES SOURCE:4 , 0.80
	TIMES SOURCE:5 , 0.80
ELSEIF ABL:12 == 2
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:12 == 3
	TIMES SOURCE:4 , 1.50
	TIMES SOURCE:5 , 1.50
ELSEIF ABL:12 == 4
	TIMES SOURCE:4 , 2.50
	TIMES SOURCE:5 , 2.50
ELSE
	TIMES SOURCE:4 , 4.00
	TIMES SOURCE:5 , 4.00
ENDIF

;獣姦の場合はここで終了
IF TEQUIP:89
	;犬の애널とファーストキス
	IF CFLAG:16 == -1
		TFLAG:13 = 1
		CFLAG:16 = 996
		;屈服刻印３に相当
		TFLAG:200 = 3
	ENDIF

	RETURN 1
ENDIF

;-------------------------------------------------
;調教者の사정ゲージチェック
;-------------------------------------------------
B = 0

;ABL:技巧をみる
IF ABL:12 == 0
	B = 100
ELSEIF ABL:12 == 1
	B = 300
ELSEIF ABL:12 == 2
	B = 500
ELSEIF ABL:12 == 3
	B = 800
ELSEIF ABL:12 == 4
	B = 1200
ELSE
	B = 1600
ENDIF

;ABL:従順をみる
IF ABL:10 == 0
	TIMES B , 0.80
ELSEIF ABL:10 == 1
	TIMES B , 0.90
ELSEIF ABL:10 == 2
	TIMES B , 1.00
ELSEIF ABL:10 == 3
	TIMES B , 1.10
ELSEIF ABL:10 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:奉仕技術をみる
IF ABL:13 == 0
	TIMES B , 0.50
ELSEIF ABL:13 == 1
	TIMES B , 0.80
ELSEIF ABL:13 == 2
	TIMES B , 1.20
ELSEIF ABL:13 == 3
	TIMES B , 1.50
ELSEIF ABL:13 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;TALENT:舌使いをみる
SIF TALENT:52
	TIMES B , 2.00

;プレイヤーのABL:Ａ感覚をみる
IF ABL:PLAYER:3 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:3 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:3 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:3 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

SIF TALENT:PLAYER:122  || TALENT:PLAYER:121
	BASE:PLAYER:2 += B

;-------------------------------------------------
;사정チェック
;-------------------------------------------------
S = BASE:PLAYER:2
EJAC = MAXBASE:PLAYER:2

IF  S > EJAC * 2
	E = 2
ELSEIF S > EJAC
	E = 1
ELSE
	E = 0
ENDIF

IF E
;사정している
	TIMES SOURCE:4 , 3.00

	;ABL:精液中毒をみる
	IF ABL:32 == 0
		SOURCE:7 = 0
		TIMES SOURCE:5 , 2.00
		TIMES SOURCE:13, 2.00
	ELSEIF ABL:32 == 1
		SOURCE:7 = 200
		TIMES SOURCE:5 , 2.50
		TIMES SOURCE:13, 1.60
	ELSEIF ABL:32 == 2
		SOURCE:7 = 500
		TIMES SOURCE:5 , 3.00
		TIMES SOURCE:13, 1.00
	ELSEIF ABL:32 == 3
		SOURCE:7 = 1200
		TIMES SOURCE:5 , 4.50
		TIMES SOURCE:13, 0.70
	ELSEIF ABL:32 == 4
		SOURCE:7 = 2500
		TIMES SOURCE:5 , 6.00
		TIMES SOURCE:13, 0.40
	ELSE
		SOURCE:7 = 5000
		TIMES SOURCE:5 , 8.00
		TIMES SOURCE:13, 0.10
	ENDIF
ENDIF

;대량사정
IF E == 2
	TIMES SOURCE:7 , 2.00
	TIMES SOURCE:5 , 1.50

	EXP:PLAYER:3 += 2
	EXP:20 += 4
	PRINTL 대량사정
	PRINTL 정액경험＋４
	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:2 -= EJAC*2
	SIF BASE:PLAYER:2 >= EJAC
		BASE:PLAYER:2 = EJAC-1

	;クンニで사정させたフラグ
	TFLAG:5 = 2
;通常の사정
ELSEIF E == 1
	EXP:PLAYER:3 += 1
	EXP:20 += 1
	PRINTL 사정
	PRINTL 정액경험＋１
	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:2 -= EJAC
	SIF BASE:PLAYER:2 >= EJAC
		BASE:PLAYER:2 = EJAC-1

	;クンニで사정させたフラグ
	TFLAG:5 = 1
ENDIF

;-------------------------------------------------
;汚れ
;-------------------------------------------------
;노예の口⇔調教者のＡの汚れが移動
STAIN:0 |= STAIN:PLAYER:4
STAIN:PLAYER:4 |= STAIN:0

;-------------------------------------------------
;経験上昇
;-------------------------------------------------
IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +6
	EXP:40 += 6
ENDIF

TFLAG:100 = 1

;屈服刻印３に相当
TFLAG:200 = 3

;ファーストキス
IF CFLAG:16 == -1
	CFLAG:16 = NO:PLAYER + 401
	CSTR:4 = %SAVESTR:PLAYER%
	TFLAG:13 = 1
ENDIF

RETURN 1
;

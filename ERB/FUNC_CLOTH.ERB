;=================================================
;의상 관련 함수군
;=================================================
;-------------------------------------------------
;使用一時フラグ
;TFLAG:45 =
;조교 중에 하의・着衣아래が穿けない状態になっている
;（&1:パンツ &2:調教後に하의処分 &4:着衣아래 &8:調教後に着衣処分 &16:特別コス &32:調教後に特別コス処分）
;-------------------------------------------------
;사용 케릭터 플래그
;CFLAG;40 = 着衣状態（&1:パンツ &2:ブラジャー &4:上着上 &8:上着아래・スカートタイプ &:16上着아래・ズボンタイプ &64:特別コス）
;CFLAG:41 = 상의의 타입
;CFLAG:42 = 特別コスチュームのタイプ
;CFLAG:43 = パンツの状態（-2:廃棄 -1:没収 0:通常 1以上:洗濯中）
;CFLAG:44 = ブラジャーの状態（-2:廃棄 -1:没収 0:通常 1以上:洗濯中）
;CFLAG:45 = 上着上の状態（-2:廃棄 -1:没収 0:通常 1以上:洗濯中）
;CFLAG:46 = 上着아래の状態（-2:廃棄 -1:没収 0:通常 1以上:洗濯中）
;CFLAG:47 = 特別コスチュームの状態（-1:没収 0:通常 1以上:洗濯中）

;-------------------------------------------------
;よく参照するシチュ早見表
;CFLAG:40 & 3  == False;:何も하의を着けていない True:何か하의を付けている
;CFLAG:40 & 6  == False:上半身裸 True:上半身に何か着ている
;CFLAG:40 & 17 == False:性器を直に触れる状態 True:性器を触れない状態
;CFLAG:40 & 24 == False:上着の아래部分を付けていない True:上着の아래部分を付けている
;CFLAG:40 & 25 == False:아래半身裸 True:아래半身に何か着ている
;CFLAG:40 & 28 == False:上着を上も아래も一切着ていない True:何か上着を着ている
;=================================================
;-------------------------------------------------
;使用中の着衣表示
;-------------------------------------------------
@PRINT_CLOTHTYPE
;着衣設定を使ってない場合
IF FLAG:37 == 0 || CFLAG:41 == 0
	PRINT 알몸 
	RETURN 1
ENDIF

;特別コス：スライムの場合の処理
IF CFLAG:42 == 11 && (CFLAG:40 & 64)
	PRINT 슬라임에 감싸진 모습 
	RETURN 1
ENDIF

;基本コスチューム処理
CALL PRINT_CLOTHTYPE_MAIN

;特別コスチューム処理
IF CFLAG:42
	PRINT 에 
	CALL PRINT_CLOTHTYPE_SPECIAL 
	PRINT 차림
ENDIF

RETURN 1

;基本コスチューム
@PRINT_CLOTHTYPE_MAIN
;ふんどしの場合
IF CFLAG:41 == 192 && (CFLAG:40 & 16)
	PRINT 훈도시 한장 
	RETURN 1
;体操着とブルマで半脱ぎの場合の処理
ELSEIF CFLAG:41 == 109
	IF (CFLAG:40 & 4) && (CFLAG:40 & 24) == 0
		PRINT 체육복 상의만 
		IF (CFLAG:40 & 1)
			PRINTFORM 팬티가 죄다 보임 
		ELSE
			PRINT 하반신 알몸 
		ENDIF
		RETURN 1
	ELSEIF (CFLAG:40 & 4) == 0 && (CFLAG:40 & 24)
		IF (CFLAG:40 & 2)
			PRINT 부르마와 브래지어 차림
		ELSE
			PRINT 부르마 한 장 
		ENDIF
		RETURN 1
	ENDIF
ENDIF
;全身タイプで半脱ぎの場合の処理
IF CFLAG:41 >= 201 && CFLAG:41 <= 300
	IF (CFLAG:40 & 4) && (CFLAG:40 & 24) == 0
		CALL PRINT_CLOTHTYPE_MAIN2
		PRINT 의 아래옷이가 찢어져, 
		IF (CFLAG:40 & 1)
			PRINTFORM 팬티가 죄다 보임 
		ELSE
			PRINT 하반신 알몸 
		ENDIF
		RETURN 1
	ELSEIF (CFLAG:40 & 4) == 0 && (CFLAG:40 & 24)
		CALL PRINT_CLOTHTYPE_MAIN2
		PRINT 의 가슴팍이 찢어져, 
		IF (CFLAG:40 & 2)
			PRINT 위는 브래지어만 
		ELSE
			IF TALENT:122 == 0 && TALENT:116 == 0 && (TALENT:109 == 0 || TALENT:132 == 0)
				PRINT 유방 노출 
			ELSE
				PRINT 상반신 알몸 
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
ENDIF
;通常の衣服
IF (CFLAG:40 & 28)
	IF (CFLAG:40 & 4) && (CFLAG:40 & 24) == 0
		CALL PRINT_CLOTHTYPE_MAIN2
		PRINT 위에만
		IF (CFLAG:40 & 1)
			PRINT 에 아래는 팬티 한 장 
		ELSE
			PRINT 에 하반신 알몸 
		ENDIF
	ELSEIF (CFLAG:40 & 4) == 0 && (CFLAG:40 & 24)
		IF (CFLAG:40 & 2)
			PRINT 브래지어에 
		ELSE
			IF TALENT:122 == 0 && TALENT:116 == 0 && (TALENT:109 == 0 || TALENT:132 == 0)
				PRINT 유방 노출에 
			ELSE
				PRINT 상반신 알몸에 
			ENDIF
		ENDIF
		CALL PRINT_CLOTHTYPE_MAIN2
		IF CFLAG:41 >= 1 && CFLAG:41 <= 100
			PRINT 의 스커트 차림
		ELSE
			PRINT 아래만 입은 차림
		ENDIF
	ELSE
		CALL PRINT_CLOTHTYPE_MAIN2
		SIF (CFLAG:40 & 64) == 0
			PRINT 차림
	ENDIF
ELSEIF (CFLAG:40 & 1) && (CFLAG:40 & 2)
	PRINT 속옷 
	SIF (CFLAG:40 & 64) == 0
		PRINT 차림
ELSEIF (CFLAG:40 & 1)
	PRINTFORM 팬티 한장 
ELSEIF (CFLAG:40 & 2)
	PRINT 브라 한 장으로 하반신을 노출 
ELSE
	PRINT 알몸 
ENDIF

RETURN 1

;-------------------------------------------------
;着衣フラグの初期化
;-------------------------------------------------
@WEARING_CLOTH_ALL
;標準衣装が設定されてない場合は戻る
SIF CFLAG:41 == 0 && CFLAG:42 == 0
	RETURN 0

;一旦全裸に
CFLAG:40 = 0

;全裸以外の場合の標準コス処理
IF CFLAG:41 != 0
	;パンツ装着
	CFLAG:40 |= 1
	;절벽, 미숙, 유치＋貧乳の場合を除きブラ装着
	SIF TALENT:116 == 0 && TALENT:135 == 0 && (TALENT:132 == 0 || TALENT:109 == 0)
		CFLAG:40 |= 2
	;和服・バニースーツ用ノーブラ化処理
	SIF (CFLAG:40 & 2) && (CFLAG:41 == 202 || CFLAG:41 == 254)
		CFLAG:40 -= 2
	;全裸の上にまとうタイプ(191～200or241～250or291～300)なら全裸にする
	SIF (CFLAG:41 >= 191 && CFLAG:41 <= 200)
		CFLAG:40 = 0
	SIF (CFLAG:41 >= 241 && CFLAG:41 <= 250)
		CFLAG:40 = 0
	SIF (CFLAG:41 >= 291 && CFLAG:41 <= 300)
		CFLAG:40 = 0
	;島の娘の服用ノーブラ・ノーパン処理
	SIF CFLAG:41 == 29
		CFLAG:40 = 0
	;オムツ着用時のノーパン処理
	SIF (CFLAG:40 & 1) && CFLAG:42 == 69
		CFLAG:40 -= 1
	
	;아래がスカートタイプのツーピース
	IF CFLAG:41 >= 1 && CFLAG:41 <= 100
		CFLAG:40 |= 4
		CFLAG:40 |= 8
	;아래がズボンタイプのツーピース
	ELSEIF CFLAG:41 >= 101 && CFLAG:41 <= 200
		CFLAG:40 |= 4
		CFLAG:40 |= 16
	;아래がスカートタイプの全身衣装
	ELSEIF CFLAG:41 >= 201 && CFLAG:41 <= 250
		CFLAG:40 |= 4
		CFLAG:40 |= 8
	;아래がズボンタイプの全身衣装
	ELSEIF CFLAG:41 >= 251 && CFLAG:41 <= 300
		CFLAG:40 |= 4
		CFLAG:40 |= 16
	ENDIF

	;ふんどし
	SIF CFLAG:41 == 192
		CFLAG:40 = 16

ENDIF

;特別コスの装着
SIF CFLAG:42
	CFLAG:40 |= 64

RETURN 1

;-------------------------------------------------
;着用可能な衣装の全装着
;-------------------------------------------------
@WEARING_CLOTH_ABLE
CALL WEARING_CLOTH_ALL
SIF CFLAG:43 != 0
	CFLAG:40 -= (CFLAG:40 & 1)
SIF CFLAG:44 != 0
	CFLAG:40 -= (CFLAG:40 & 2)
SIF CFLAG:45 != 0
	CFLAG:40 -= (CFLAG:40 & 4)
SIF CFLAG:46 != 0
	CFLAG:40 -= (CFLAG:40 & 8)
SIF CFLAG:46 != 0
	CFLAG:40 -= (CFLAG:40 & 16)
SIF CFLAG:47 != 0
	CFLAG:40 -= (CFLAG:40 & 64)

;-------------------------------------------------
;調教後の衣類の処理
;-------------------------------------------------
@AFTERTRAIN_CLOTH
;汚れた衣類の始末
S = 0
;特別コス
IF CFLAG:42 && (TFLAG:45 & 32)
	PRINTFORM （%SAVESTR:TARGET%의 
	CALL PRINT_CLOTHTYPE_SPECIAL 
	PRINTL 은 폐기처분 되었습니다）
	CFLAG:42 = 0
	TFLAG:45 -= 32
	S = 1
	SIF CFLAG:40 & 64
		CFLAG:40 -= 64
ELSEIF CFLAG:42 == 69 && (TFLAG:45 & 16) && CFLAG:47 == 0 && MONEY >= 50
	;オムツの場合の特殊処理
	$INPUT_LOOP_01
	PRINTFORML 50포인트로 %SAVESTR:TARGET%의 기저귀를 갈아 줍니까？
	PRINTL  [0] - 네
	PRINTL  [1] - 아니오
	INPUT
	IF RESULT == 0
		PRINTFORM （%SAVESTR:TARGET%에게 새로운 기저귀를 입혀 주었습니다）
		MONEY -= 50
		CFLAG:47 = 0
		TFLAG:45 -= 16
		IF TALENT:135 == 0
			PRINTL 
			PRINTFORM %PALAMNAME:8%의 구슬＋500
			JUEL:8 += 500
		ENDIF
		WAIT
		S = 1
	ELSEIF RESULT == 1
		PRINTFORMW （%SAVESTR:TARGET%의 기저귀를 세탁소에 보냈습니다）
		CFLAG:47 = 2
		TFLAG:45 -= 16
		S = 1
		SIF CFLAG:40 & 64
			CFLAG:40 -= 64
	ELSE
		GOTO INPUT_LOOP_01
	ENDIF
ELSEIF CFLAG:42 && (TFLAG:45 & 16) && CFLAG:47 == 0
	PRINTFORM （%SAVESTR:TARGET%의 
	CALL PRINT_CLOTHTYPE_SPECIAL 
	PRINTW 을 세탁소에 보냈습니다）
	CFLAG:47 = 5
	;オムツの場合は洗濯速度は하의並
	SIF CFLAG:42 == 69
		CFLAG:47 = 2
	TFLAG:45 -= 16
	S = 1
	SIF CFLAG:40 & 64
		CFLAG:40 -= 64
ENDIF
;上着아래
IF CFLAG:41 && (TFLAG:45 & 8)
	PRINTFORM （%조사선택(SAVESTR:TARGET, "가")% 입고 있던 
	CALL PRINT_CLOTHTYPE_MAIN2
	IF CFLAG:41 >= 1 && CFLAG:41 <= 100
		PRINT 의 스커트
	ELSEIF CFLAG:41 <= 200
		PRINT 아래
	ENDIF
	PRINTW 는 폐기처분 되었습니다）
	IF CFLAG:41 >= 201
		CFLAG:41 = 0
		SIF CFLAG:40 & 4
			CFLAG:40 -= 4
		SIF CFLAG:40 & 8
			CFLAG:40 -= 8
		SIF CFLAG:40 & 16
			CFLAG:40 -= 16
	ELSE
		CFLAG:46 = -2
		SIF CFLAG:40 & 8
			CFLAG:40 -= 8
		SIF CFLAG:40 & 16
			CFLAG:40 -= 16
	ENDIF
	TFLAG:45 -= 8
	S = 1
ELSEIF CFLAG:41 && (TFLAG:45 & 4) && CFLAG:46 == 0
	PRINTFORM （%조사선택(SAVESTR:TARGET, "가")% 입고 있던 
	CALL PRINT_CLOTHTYPE_MAIN2
	IF CFLAG:41 >= 1 && CFLAG:41 <= 100
		PRINT 의 스커트
	ELSEIF CFLAG:41 <= 200
		PRINT 아래
	ENDIF
	PRINTW 을 세탁소에 보냈습니다）
	IF CFLAG:41 >= 201
		CFLAG:45 = 3
		CFLAG:46 = 3
		SIF CFLAG:40 & 4
			CFLAG:40 -= 4
		SIF CFLAG:40 & 8
			CFLAG:40 -= 8
		SIF CFLAG:40 & 16
			CFLAG:40 -= 16
	ELSE
		CFLAG:46 = 3
		SIF CFLAG:40 & 8
			CFLAG:40 -= 8
		SIF CFLAG:40 & 16
			CFLAG:40 -= 16
	ENDIF
	TFLAG:45 -= 4
	S = 1
ENDIF
;パンツ
IF (TFLAG:45 & 2)
	PRINTFORMW （%SAVESTR:TARGET%의 속옷은 폐기처분 되었습니다）
	CFLAG:43 = -2
	SIF CFLAG:40 & 1
		CFLAG:40 -= 1
	TFLAG:45 -= 2
	S = 1
ELSEIF (TFLAG:45 & 1) && CFLAG:43 == 0
	PRINTFORMW （%SAVESTR:TARGET%의 속옷을 세탁소에 보냈습니다）
	CFLAG:43 = 2
	SIF CFLAG:40 & 1
		CFLAG:40 -= 1
	TFLAG:45 -= 1
	S = 1
ENDIF

;上아래ともダメになった衣装は削除
IF CFLAG:41
	SIF CFLAG:45 < 0 && CFLAG:46 < 0
		CFLAG:41 = 0
	;ふんどし用処理
	SIF CFLAG:41 == 192 && CFLAG:46 < 0
		CFLAG:41 = 0
ENDIF
;ダメになった特別コスは削除
IF CFLAG:42
	SIF CFLAG:47 < 0
		CFLAG:42 = 0
ENDIF

RETURN 1

;-------------------------------------------------
;衣類の再着衣
;-------------------------------------------------
@RE_CLOTHED
;순종＋노출벽 3이상이라면, 主人に脱がされた状態を維持
;それ以外なら着られる衣類は全部身に着ける
IF ABL:10 + ABL:17 < 3
	C = CFLAG:40
	CALL WEARING_CLOTH_ABLE
	IF CFLAG:40 > C
		PRINTFORML （%조사선택(SAVESTR:TARGET, "는")%, 벗겨져 있던 의상을 다시 몸에 걸쳤습니다）
		WAIT
	ENDIF
ENDIF

RETURN 1

;-------------------------------------------------
;더러워진 의복 세탁
;-------------------------------------------------
@WASHING_CLOTH
S = 0
REPEAT CHARANUM
	TARGET = COUNT
	;全身衣装の場合, 洗濯の進行状況は아래半身部分にあわせる
	SIF CFLAG:41 >= 200 && CFLAG:45 > 0 && CFLAG:46 > 0
		CFLAG:45 = CFLAG:46
	;하의の洗濯
	IF CFLAG:43 > 0
		CFLAG:43 -= 1
		IF CFLAG:43 == 0
			PRINTFORML （%SAVESTR:TARGET%의 하의가 세탁으로부터 돌아왔습니다）
			S = 1
		ENDIF
	ENDIF
	;通常コスの洗濯
	IF CFLAG:46 > 0
		CFLAG:46 -= 1
		IF CFLAG:46 == 0 && CFLAG:41
			PRINTFORM （%SAVESTR:TARGET%의 
			CALL PRINT_CLOTHTYPE_MAIN2
			PRINTFORML 가 세탁소로부터 돌아왔습니다）
			S = 1
			SIF CFLAG:41 >= 200 && CFLAG:45 >= 0
				CFLAG:45 = 0
		ENDIF
	ENDIF
	;特別コスの洗濯
	IF CFLAG:47 > 0
		CFLAG:47 -= 1
		IF CFLAG:47 == 0 && CFLAG:42
			PRINTFORM （%SAVESTR:TARGET%의 
			CALL PRINT_CLOTHTYPE_SPECIAL 
			PRINTFORML 가 세탁소로부터 돌아왔습니다）
			S = 1
		ENDIF
	ENDIF

	;パンツのはき古し進行もここで
	SIF (CFLAG:40 & 1)
		CFLAG:48 += 1

REND
IF S
	WAIT
	DRAWLINE
ENDIF

RETURN 1

;-------------------------------------------------
;조교중のおもらし処理（小）
;-------------------------------------------------
@SOILING_CLOTH_NO1
;着衣設定でなければそのまま終了
SIF FLAG:37 == 0
	RETURN 0

;着衣中に放尿した場合, 着衣は一時的に使えなくなる
IF (CFLAG:40 & 64) && (CFLAG:42 <= 50 || CFLAG:42 == 69)
	PRINTFORM 《%SAVESTR:TARGET%의 
	CALL PRINT_CLOTHTYPE_SPECIAL 
	PRINTFORML (은)는 오줌 투성이가 되었다》
	TFLAG:45 |= 16
	;オムツ着用中なら他の衣類は無事
	SIF CFLAG:42 == 69
		RETURN 1
ENDIF
IF (CFLAG:40 & 8) || (CFLAG:40 & 16)
	PRINTFORM 《%조사선택(SAVESTR:TARGET, "가")% 입고 있던 
	CALL PRINT_CLOTHTYPE_MAIN2
	IF CFLAG:41 >= 1 && CFLAG:41 <= 100
		PRINT 의 스커트
	ELSEIF CFLAG:41 <= 200
		PRINT 아래
	ENDIF
	PRINTFORML (은)는 오줌 투성이가 되었다》
	TFLAG:45 |= 4
ENDIF
IF (CFLAG:40 & 1)
	PRINTFORML 《%SAVESTR:TARGET%의 속옷은 오줌투성이가 되었다》
	TFLAG:45 |= 1
ENDIF

RETURN 1

;-------------------------------------------------
;조교중のおもらし処理（大）
;-------------------------------------------------
@SOILING_CLOTH_NO2
;着衣設定でなければそのまま終了
SIF FLAG:37 == 0
	RETURN 0

;着衣中に脱糞した場合, 着衣は廃棄処分になる
IF (CFLAG:40 & 64) && (CFLAG:42 <= 50 || CFLAG:42 == 69)
	PRINTFORM 《%SAVESTR:TARGET%의 
	CALL PRINT_CLOTHTYPE_SPECIAL 
	PRINTL (은)는 오물 투성이가 되었다
	TFLAG:45 |= 16
	TFLAG:45 |= 32
	;オムツ着用中なら他の衣類は無事
	SIF CFLAG:42 == 69
		RETURN 1
ENDIF
IF (CFLAG:40 & 8) || (CFLAG:40 & 16)
	PRINTFORM 《%조사선택(SAVESTR:TARGET, "가")% 입고 있던 
	CALL PRINT_CLOTHTYPE_MAIN2
	IF CFLAG:41 >= 1 && CFLAG:41 <= 100
		PRINT 의 스커트
	ELSEIF CFLAG:41 <= 200
		PRINT 아래
	ENDIF
	PRINTL 는 오물투성이가 되었다》
	TFLAG:45 |= 4
	TFLAG:45 |= 8
ENDIF
IF (CFLAG:40 & 1)
	PRINTFORML 《%SAVESTR:TARGET%의 속옷은 오물투성이가 되었다》
	TFLAG:45 |= 1
	TFLAG:45 |= 2
ENDIF

RETURN 1


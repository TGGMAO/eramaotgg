@ORC_MARRIAGE_DAY,ARG
;--------------------------------------------

PRINTFORMW %SAVESTR:ARG%는 아인종과 동굴에서 살고 있다

IF A == 0
	;あなたの場合
	IF RAND:2 == 0
		IF TALENT:(ARG):273 == 1 || (CFLAG:(ARG):42 == 79 && (CFLAG:(ARG):40 & 64) && FLAG:37) || TALENT:(ARG):122 == 1
			PRINTFORMW %SAVESTR:ARG%는 아인종들의 정액 처리에 기쁨을 느끼고 있는 것 같다……
			PRINTFORML A경험+{Y}
			PRINTFORML 펠라경험+{Y}
			PRINTFORML 정액 경험+{Y}
			EXP:(ARG):0 += Y
			EXP:(ARG):22 += Y
			EXP:(ARG):20 += Y
		ELSE
			PRINTFORMW %SAVESTR:ARG%는 아인종들의 정액 처리에 기쁨을 느끼고 있는 것 같다……
			PRINTFORML V경험+{Y}
			PRINTFORML A경험+{Y}
			PRINTFORML 펠라경험+{Y}
			PRINTFORML 정액 경험+{Y}
			EXP:(ARG):0 += Y
			EXP:(ARG):1 += Y
			EXP:(ARG):22 += Y
			EXP:(ARG):20 += Y
			CFLAG:(ARG):107 += Y
		ENDIF
	ELSE
		PRINTFORMW %SAVESTR:ARG%는 아인종들에게 사랑을 느끼고 있다……
		PRINTFORML 펠라경험+{Y}
		PRINTFORML 정액 경험+{Y}
		EXP:(ARG):22 += Y
		EXP:(ARG):20 += Y
	ENDIF
ELSEIF CFLAG:(ARG):602 > 40
	;40日後
	IF RAND:2 == 0
		;性交系
		PRINTFORM %SAVESTR:ARG%는, 
		;露出癖3以上
		SIF ABL:(ARG):17 >= 3
			PRINT 남편의 동료로 보면서
		;60日後
		SIF CFLAG:(ARG):602 > 60
			PRINT 익숙한 모습으로
		IF TALENT:(ARG):273 == 1 || (CFLAG:(ARG):42 == 79 && (CFLAG:(ARG):40 & 64) && FLAG:37) || TALENT:(ARG):122 == 1
			PRINT 아날
		ELSE
			PRINT 보지
		ENDIF
		PRINT 를 열고,  아인종 남편의
		IF RAND:2 == 0
			PRINT 성욕처리
		ELSE
			PRINT 정액 처리
		ENDIF
		PRINTW 로 기쁨을 느끼고 있었다……
		IF TALENT:(ARG):273 == 1 || (CFLAG:(ARG):42 == 79 && (CFLAG:(ARG):40 & 64) && FLAG:37) || TALENT:(ARG):122 == 1
			PRINTFORML A경험+{Y}
			PRINTFORML 펠라경험+{Y}
			PRINTFORML 정액 경험+{Y}
			EXP:(ARG):0 += Y
			EXP:(ARG):22 += Y
			EXP:(ARG):20 += Y
		ELSE
			PRINTFORML V경험+{Y}
			PRINTFORML A경험+{Y}
			PRINTFORML 펠라경험+{Y}
			PRINTFORML 정액 경험+{Y}
			EXP:(ARG):0 += Y
			EXP:(ARG):1 += Y
			EXP:(ARG):22 += Y
			EXP:(ARG):20 += Y
			CFLAG:(ARG):107 += Y
		ENDIF
	ELSE
		PRINTFORMW %SAVESTR:ARG%는 아인종들에게 사랑을 느끼고 있다……
		PRINTFORML 펠라경험+{Y}
		PRINTFORML 정액 경험+{Y}
		EXP:(ARG):22 += Y
		EXP:(ARG):20 += Y
	ENDIF
ELSEIF ABL:(ARG):30 >= 1
	;성교중독
	IF TALENT:(ARG):273 == 1 || (CFLAG:(ARG):42 == 79 && (CFLAG:(ARG):40 & 64) && FLAG:37) || TALENT:(ARG):122 == 1
		PRINTFORMW %SAVESTR:ARG%는 아인종과의 항문 성교에 기쁨을 느끼고 있다……
		PRINTFORML A경험+{Y}
		PRINTFORML 펠라경험+{Y}
		PRINTFORML 정액 경험+{Y}
		EXP:(ARG):1 += Y
		EXP:(ARG):22 += Y
		EXP:(ARG):20 += Y
	ELSE
		PRINTFORMW %SAVESTR:ARG%는 아인종과의 성교에 기쁨을 느끼고 있다……
		PRINTFORML V경험+{Y}
		PRINTFORML 펠라경험+{Y}
		PRINTFORML 정액 경험+{Y}
		EXP:(ARG):0 += Y
		EXP:(ARG):22 += Y
		EXP:(ARG):20 += Y
		CFLAG:(ARG):107 += Y

;========================================
;                        조사선택
;
;  모든 개별 함수의 BASE 함수
;  입력받은 이름에 해당 조사를 붙여 되돌려준다
;
;    ex) SAVESTR:MASTER가 '당신'인 경우, 
;        %조사선택(SAVESTR:MASTER,"는")%
;         를 부르면 '당신은' 이 반환된다
;
;  종류 : 式中関数
;  PRINTFORM 등의 출력 함수에서 부르면 내용이 전개되어 반환된다
;========================================


;========================================
;                        조사선택 (ver.2)
;
;  입력받은 이름에 따라 조사를 처리하는 함수
;
;    ex) %조사선택(SAVESTR:TARGET,"이")%
;
;  式中関数 등에도 대응할 수 있다
;
;    ex) %조사선택(CNAME(),"를")%
;
;========================================

@조사선택(ARGS, ARGS:2)
#FUNCTIONS

;이름을 받아온다
LOCALS = %ARGS%

;받침 판정 및 조사 선택
IF KR_NAME(LOCALS) == 2
	;받침 없음. ㄴ/ㄹ
	IF ARGS:2 == "는" || ARGS:2 == "은"
		LOCALS += "는"
	ELSEIF ARGS:2 == "가" || ARGS:2 == "이"
		LOCALS += "가"
	ELSEIF ARGS:2 == "를" || ARGS:2 == "을"
		LOCALS += "를"
	ELSEIF ARGS:2 == "와" || ARGS:2 == "과"
		LOCALS += "와"
	ELSEIF ARGS:2 == "로" || ARGS:2 == "으로"
		LOCALS += "로"
	ELSEIF ARGS:2 == "랑" || ARGS:2 == "이랑"
		LOCALS += "랑"
	ELSEIF ARGS:2 == "라" || ARGS:2 == "이라"
		LOCALS += "라"
	ELSEIF ARGS:2 == "며" || ARGS:2 == "이며"
		LOCALS += "며"
	ELSEIF ARGS:2 == "고" || ARGS:2 == "이고"
		LOCALS += "고"
	ELSEIF ARGS:2 == "다" || ARGS:2 == "이다"
		LOCALS += "다"
	ELSEIF ARGS:2 == "였" || ARGS:2 == "이었"
		LOCALS += "였"
	ELSEIF ARGS:2 == "여" || ARGS:2 == "이여"
		LOCALS += "여"
	ELSEIF ARGS:2 == "야" || ARGS:2 == "이야"
		LOCALS += "야"
	ELSEIF ARGS:2 == "나" || ARGS:2 == "이나"
		LOCALS += "나"
	ELSEIF ARGS:2 == "면" || ARGS:2 == "이면"
		LOCALS += "면"
	ELSEIF ARGS:2 == "지만" || ARGS:2 == "이지만"
		LOCALS = "지만"
	ENDIF
ELSE
	;받침 없음. ㅇ
	IF ARGS:2 == "는" || ARGS:2 == "은"
		LOCALS += "은"
	ELSEIF ARGS:2 == "가" || ARGS:2 == "이"
		LOCALS += "이"
	ELSEIF ARGS:2 == "를" || ARGS:2 == "을"
		LOCALS += "을"
	ELSEIF ARGS:2 == "와" || ARGS:2 == "과"
		LOCALS += "과"
	ELSEIF ARGS:2 == "로" || ARGS:2 == "으로"
		LOCALS += "으로"
	ELSEIF ARGS:2 == "랑" || ARGS:2 == "이랑"
		LOCALS += "이랑"
	ELSEIF ARGS:2 == "라" || ARGS:2 == "이라"
		LOCALS += "이라"
	ELSEIF ARGS:2 == "며" || ARGS:2 == "이며"
		LOCALS += "이며"
	ELSEIF ARGS:2 == "고" || ARGS:2 == "이고"
		LOCALS += "이고"
	ELSEIF ARGS:2 == "다" || ARGS:2 == "이다"
		LOCALS += "이다"
	ELSEIF ARGS:2 == "였" || ARGS:2 == "이었"
		LOCALS += "이었"
	ELSEIF ARGS:2 == "여" || ARGS:2 == "이여"
		LOCALS += "이여"
	ELSEIF ARGS:2 == "야" || ARGS:2 == "이야"
		LOCALS += "이야"
	ELSEIF ARGS:2 == "나" || ARGS:2 == "이나"
		LOCALS += "이나"
	ELSEIF ARGS:2 == "면" || ARGS:2 == "이면"
		LOCALS += "이면"
	ELSEIF ARGS:2 == "지만" || ARGS:2 == "이지만"
		LOCALS = "이지만"
	ENDIF
ENDIF

LOCALS = %후치환(LOCALS)%

RETURNF LOCALS



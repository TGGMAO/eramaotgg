	PRINT 조수가 딸
	PRINTV '(,20,')
	S = 1
ELSEIF TFLAG:14 == 3
	PRINT  - 
	A -= 5
	PRINT 조수가 언니
	PRINTV '(,5,')
	S = 1
ELSEIF TFLAG:14 == 4
	PRINT  - 
	A -= 5
	PRINT 조수가 여동생
	PRINTV '(,5,')

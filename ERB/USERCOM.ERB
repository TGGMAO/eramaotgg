;SHOW_USERCOM
;FLAG:25 & 1 애무계 필터
;FLAG:25 & 2 기구계 필터
;FLAG:25 & 4 Ａ성교계 필터
;FLAG:25 & 8 Ｖ성교계 필터
;FLAG:25 & 16 ＳＭ계 필터
@SHOW_USERCOM
resetcolor
PRINTC 능력 표시[100]
PRINTC 더러움 표시[101]
IF ASSI > 0
	PRINTC 조수와 교대[102]
	PRINTL 
ENDIF
PRINTC 콘돔 설정[103]
printl
IF FLAG:25 & 1
	SETCOLOR 100,100,100
	PRINTC 애무계 필터[104]
	resetcolor
ELSE
	PRINTC 애무계 필터[104]
ENDIF
IF FLAG:25 & 2
	SETCOLOR 100,100,100
	PRINTC 기구계 필터[105]
	resetcolor
ELSE
	setcolor 100,149,237
	PRINTC 기구계 필터[105]
	resetcolor
ENDIF
IF FLAG:25 & 4
	SETCOLOR 100,100,100
	PRINTC Ｖ성교계 필터[106]
	resetcolor
ELSE
	setcolor 255,165,0
	PRINTC Ｖ성교계 필터[106]
	resetcolor
ENDIF
PRINTL 
IF FLAG:25 & 8
	SETCOLOR 100,100,100
	PRINTC Ａ성교계 필터[107]
	resetcolor
ELSE
	setcolor 219,112,147
	PRINTC Ａ성교계 필터[107]
	resetcolor
ENDIF
IF FLAG:25 & 16
	SETCOLOR 100,100,100
	PRINTC ＳＭ계 필터[108]
	resetcolor
ELSE
	setcolor 255,99,71
	PRINTC ＳＭ계 필터[108]
	resetcolor
ENDIF
PRINTC 조교 종료[200]
printc
printl
IF PREVCOM > -1
	CALL P_C
	PRINT ＜전회 조교 커맨드：
	SETCOLOR 0xDDA0DD
	PRINTFORM %TSTR:90%
	RESETCOLOR
	PRINTL ＞
ENDIF

@USERCOM
IF RESULT == 100
	CALL SHOW_CHARA_INFO(TARGET)
	RETURN 1
ELSEIF RESULT == 101
	CALL STAIN_INFO
	RETURN 1
ELSEIF RESULT == 102 && ASSI > 0
	IF ASSIPLAY
		ASSIPLAY = 0
		PLAYER = MASTER
	ELSE
		ASSIPLAY = 1
		PLAYER = ASSI
	ENDIF
	RETURN 1
ELSEIF RESULT == 103
	CALL CONDOM_SETTINGS
	RETURN 1
ELSEIF RESULT == 104
	IF FLAG:25 & 1
		FLAG:25 &= 30
	ELSE
		FLAG:25 |= 1
	ENDIF
ELSEIF RESULT == 105
	IF FLAG:25 & 2
		FLAG:25 &= 29
	ELSE
		FLAG:25 |= 2
	ENDIF
ELSEIF RESULT == 106
	IF FLAG:25 & 4
		FLAG:25 &= 27
	ELSE
		FLAG:25 |= 4
	ENDIF
ELSEIF RESULT == 107
	IF FLAG:25 & 8
		FLAG:25 &= 23
	ELSE
		FLAG:25 |= 8
	ENDIF
ELSEIF RESULT == 108
	IF FLAG:25 & 16
		FLAG:25 &= 15
	ELSE
		FLAG:25 |= 16
	ENDIF

ELSEIF RESULT == 200
	BEGIN AFTERTRAIN
	RETURN 1
ENDIF
RETURN 0

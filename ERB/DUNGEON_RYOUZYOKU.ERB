PRINTL  
DRAWLINE
CALL SHOW_DATA,ARG
PRINTL  

PRINTL [0] - 능욕을 본다
PRINTL [1] - 능욕하지 않는다
$INPUT_LOOP
INPUT
IF RESULT < 0
	GOTO INPUT_LOOP
ELSEIF RESULT >= 2
	GOTO INPUT_LOOP
ENDIF
SIF RESULT == 1
	RETURN 0

;凌辱畏怖記憶があるか
MON_COUNT = 0
MON_FEAR = CFLAG:ARG:130
WHILE MON_COUNT < 300
	LOCAL = MON_COUNT + 99
	IF E:LOCAL <= 0
		LOCAL = MON_COUNT + 7
		E:LOCAL = 0
	ENDIF
	LOCAL = MON_COUNT + 7
	;IF E:LOCAL > 0
		;LOCAL:1 = E:MON_COUNT
		;被凌辱モンスターID記憶を見る
		;SIF CFLAG:ARG:130 == LOCAL:1
			;MON_FEAR = E:MON_COUNT
	;ENDIF
	MON_COUNT += 100
WEND


TARGET = ARG
CALL DUNGEON_RYOUZYOKU

MON_COUNT = 0
WHILE MON_COUNT < 300
	LOCAL = MON_COUNT + 7
	LOCAL:1 = E:MON_COUNT
	IF E:LOCAL > 0 && MON_FEAR == 0
		CALL MONSTER_NAME,LOCAL:1,0
		PRINTFORMW 의 능욕이 시작됐다
		;凌辱畏怖記憶
		CFLAG:ARG:130 = LOCAL:1
		MON_FEAR = LOCAL:1
		CFLAG:ARG:131 = 0
	ELSEIF E:LOCAL > 0 && MON_FEAR == LOCAL:1
		CALL MONSTER_NAME,LOCAL:1,0
		PRINTFORMW 의 능욕이 다시 시작됐다
		CFLAG:ARG:131++
		FEAR = 1
	ELSEIF E:LOCAL > 0 && MON_FEAR != LOCAL:1
		CALL MONSTER_NAME,LOCAL:1,0
		PRINTFORMW 의 능욕이 시작됐다
		FEAR = 0
	ENDIF
	B = MON_COUNT
	IF E:LOCAL == 1
		;カタコト
		CALL ORC_RYOU,ARG,FEAR
	ELSEIF E:LOCAL == 2
		;슬라임
		CALL SLIME_RYOU,ARG,FEAR
	ELSEIF E:LOCAL == 3
		;곤충
		CALL INSECT_RYOU,ARG,FEAR
	ELSEIF E:LOCAL == 4
		;蔦촉수
		CALL IVY_RYOU,ARG,FEAR
	ELSEIF E:LOCAL == 5
		;촉수
		CALL SYOKUSYU_RYOU,ARG,FEAR
	ELSEIF E:LOCAL == 6
		;요정
		CALL FAILY_RYOU,ARG,FEAR
	ELSEIF E:LOCAL == 7
		;거인
		CALL GIANT_RYOU,ARG,FEAR
	ELSEIF E:LOCAL == 8
		;남성
		CALL MAN_RYOU,ARG,FEAR
	ELSEIF E:LOCAL == 9
		;여성
		CALL GIRL_RYOU,ARG,FEAR
	ELSEIF E:LOCAL == 10
		;짐승
		CALL BEAST_RYOU,ARG,FEAR
	ELSEIF E:LOCAL == 11
		;뇌간
		CALL BRAIN_RYOU,ARG,FEAR
	ELSEIF E:LOCAL == 12
		;말
		CALL HORSE_RYOU,ARG,FEAR
	ENDIF
	PRINTL  
	MON_COUNT += 100
WEND

IF EXP:0 > 0 && TALENT:0 == 1
	TALENT:0 = 0
	PRINTL 【처녀상실】
	CFLAG:15 = 104
ENDIF

CALL DUNGEON_RYOUZYOKU_AFTER

RETURN 0


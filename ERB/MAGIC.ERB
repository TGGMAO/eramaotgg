	IF CFLAG:A:11 < 0
		SIF FLAG:5 & 32
			PRINTW 용사는 완전히 잠들어 있다…
		CFLAG:A:11 = 0
	ELSEIF DAMAGE <= 0
		SIF FLAG:5 & 32
			PRINTW 주문이 흐지부지 되어버렸다
	ELSE
		SIF FLAG:5 & 32
			PRINTW 용사는 자고있다…
	ENDIF
	RETURN 0
ELSEIF TARGET_TYPE == 3
	SIF FLAG:5 & 32
		PRINTFORMW %조사선택(SAVESTR:B, "는")% 수면 주문을 시전했다! 
	BASE:B:1 -= 30
	DAMAGE = CFLAG:B:9
	
	CALL MAGIC_BONUS_C_TO_C,B,DAMAGE,A
	DAMAGE = RESULT
	
	;効果量キャップ
	CALL MAGIC_DAMAGE_CAP,CFLAG:B:9,CFLAG:A:9,DAMAGE,300
	DAMAGE = RESULT

	CFLAG:A:11 -= RAND:DAMAGE
	IF CFLAG:A:11 < 0
		SIF FLAG:5 & 32
			PRINTW 노예는 완전히 잠들어 있다…
		CFLAG:A:11 = 0
	ELSEIF DAMAGE <= 0
		SIF FLAG:5 & 32
			PRINTW 주문이 흐지부지 되어버렸다
	ELSE
		SIF FLAG:5 & 32
			PRINTW 노예는 자고 있다…
	ENDIF
	RETURN 0
ELSEIF TARGET_TYPE == 4
	SIF FLAG:5 & 32
		PRINTFORMW %조사선택(SAVESTR:A, "는")% 수면 주문을 시전했다! 
	BASE:A:1 -= 30
	DAMAGE = CFLAG:A:9
	
	CALL MAGIC_BONUS_C_TO_C,A,DAMAGE,B
	DAMAGE = RESULT

	;効果量キャップ
	CALL MAGIC_DAMAGE_CAP,CFLAG:A:9,CFLAG:B:9,DAMAGE,300
	DAMAGE = RESULT

	CFLAG:B:11 -= RAND:DAMAGE
	IF CFLAG:B:11 < 0
		SIF FLAG:5 & 32
			PRINTW 용사는 완전히 잠들어 있다…
		CFLAG:B:11 = 0
	ELSEIF DAMAGE <= 0
		SIF FLAG:5 & 32
			PRINTW 주문이 흐지부지 되어버렸다
	ELSE
		SIF FLAG:5 & 32
			PRINTW 용사는 자고있다…
	ENDIF
	RETURN 0
ENDIF

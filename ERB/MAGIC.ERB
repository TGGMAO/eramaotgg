;------------------------------------
@ENERGY_BOLT_MAGIC,TARGET_TYPE
#DIM TARGET_TYPE
#DIM DAMAGE
#DIM KILL_MONS
;------------------------------------
;마법 화살
IF TARGET_TYPE == 1
	SIF FLAG:5 & 32
		PRINTFORMW %조사선택(SAVESTR:A, "는")% 마법 화살 주문을 시전했다! 
	BASE:A:1 -= 15
	DAMAGE = CFLAG:A:9 * 5
	
	CALL MAGIC_BONUS_C_TO_M,A,DAMAGE
	DAMAGE = RESULT
	
	;モンスターレベルによる効果軽減
	DAMAGE -= E:(B + 1) / 20
	
	;効果量キャップ
	CALL MAGIC_DAMAGE_CAP,CFLAG:A:9,E:(B + 1),DAMAGE,600
	DAMAGE = RESULT

	KILL_MONS = DAMAGE / E:(B + 3)
	
	IF KILL_MONS <= 0
		SIF FLAG:5 & 32
			PRINTW 마법 화살 주문은 전혀 먹히지 않은것 같다
	ELSEIF KILL_MONS > 0
		SIF DAMAGE > E:(B + 99)
			DAMAGE = E:(B + 99)
		SIF FLAG:5 & 32
			PRINTFORMW 마법 화살 주문은 {KILL_MONS}마리의 몬스터를 꿰뚫었다! 
		E:(B + 99) -= KILL_MONS
		EXP:A:80 += E:(B + 1) * KILL_MONS
	ENDIF
	RETURN 0

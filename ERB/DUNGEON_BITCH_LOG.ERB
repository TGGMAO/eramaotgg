;売春用各種文字列取得関数
@FS_BITCH(TYPE, ARG)
#FUNCTIONS
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS TYPE
#DIM DICE
SELECTCASE TYPE
;数値→プレイ内容変換
;関数の利用に
CASE "PLAY"
	SELECTCASE ARG
	CASE 1
		LOCALS = HAND
	CASE 2
		LOCALS = ORAL
	CASE 3
		LOCALS = LES
	CASE 4
		LOCALS = ANAL
	CASE 5
		LOCALS = SEX
	;ダンジョン内では売春扱いではない
	CASE 6
		LOCALS = ANIMAL
	;売春では使わない
	CASE 7
		LOCALS = SELF
	CASEELSE
		$ERROR_ARG
		THROW 불분명한 인수{ARG}
	ENDSELECT
;表示用
CASE "PLAYNAME"
	SELECTCASE ARG
	CASE 1
		LOCALS = 자위매춘
	CASE 2
		LOCALS = 구음매춘
	CASE 3
		LOCALS = 레즈매춘
	CASE 4
		LOCALS = 항교매춘
	CASE 5
		LOCALS = 성교매춘
	CASE 6
		LOCALS = 수교매춘
	CASEELSE
		GOTO ERROR_ARG
	ENDSELECT
CASE "TOWN_MAN"
	SELECTCASE ARG
	CASE 1
		LOCALS = 마을의남자
	CASE 2
		LOCALS = 남자모험가
	CASE 3
		LOCALS = 마을의소년
	CASE 4
		LOCALS = 거리의졸부
	CASE 5
		LOCALS = 행상인남자
	CASEELSE
		GOTO ERROR_ARG
	ENDSELECT
CASE "TOWN_GIRL"
	SELECTCASE ARG
	CASE 1
		LOCALS = 주부
	CASE 2
		LOCALS = 여자모험가
	CASE 3
		LOCALS = 여거지
	CASE 4
		LOCALS = 미행중의귀부인
	CASE 5
		LOCALS = 여자연예인
	CASEELSE
		GOTO ERROR_ARG
	ENDSELECT
CASE "DUNGEON_MAN"
	SELECTCASE ARG
	CASE 1
		LOCALS = 오크
	CASE 2
		LOCALS = 마족의남자
	CASE 3
		LOCALS = 마족의소년
	CASE 4
		LOCALS = 마족의졸부
	CASE 5
		LOCALS = 코볼트상인
	CASEELSE
		GOTO ERROR_ARG
	ENDSELECT
CASE "DUNGEON_GIRL"
	SELECTCASE ARG
	CASE 1
		LOCALS = 음마
	CASE 2
		LOCALS = 마족의여자
	CASE 3
		LOCALS = 엘프여자거지
	CASE 4
		LOCALS = 마족의귀부인
	CASE 5
		LOCALS = 마족여자사제
	CASEELSE
		GOTO ERROR_ARG
	ENDSELECT
;本人の描写
;ABLカンスト系に再現性が無いので注意
CASE "LOOKS"
	;ランダムで上書きさせることで
	;重複した素質が入れ替わりで出現するように
	
	;本当はDICEの数を増やして確率を公平にしたいけど
	;美しくないのでこの形に
	
	DICE = 2
	;一回も上書きされない場合髪色が出る
	LOCALS = %GET_LOOK_INFO(ARG, "머리색")%의 

	SIF TALENT:ARG:253 && RAND:DICE == 0
		LOCALS = 구리빛피부의 
	SIF TALENT:ARG:255 && RAND:DICE == 0
		LOCALS = 흰 피부의 
	SIF TALENT:ARG:244 && RAND:DICE == 0
		LOCALS = 푸른피부의 

	SIF TALENT:ARG:음모의상태 == 1 && RAND:DICE == 0
		LOCALS = 백보지의
	SIF TALENT:ARG:음모의상태 > 150 && RAND:DICE == 0
		LOCALS = 음모가 짙은

	SIF TALENT:ARG:버릇 == 7 && RAND:DICE == 0
		LOCALS = 침착성이 없는 
	SIF TALENT:ARG:버릇 == 9 && RAND:DICE == 0
		LOCALS = 눈을 치켜뜨고있는 
	SIF TALENT:ARG:버릇 == 10 && RAND:DICE == 0
		LOCALS = 고개를 기울이고 
	SIF TALENT:ARG:버릇 == 11 && RAND:DICE == 0
		LOCALS = 우울해보이는 
	SIF TALENT:ARG:버릇 == 14 && RAND:DICE == 0
		LOCALS = 뾰로통한 얼굴의 
	SIF TALENT:ARG:버릇 == 18 && RAND:DICE == 0
		LOCALS = 나른한듯한 
	SIF TALENT:ARG:버릇 == 19 && RAND:DICE == 0
		LOCALS = 어딘가 불만어린 

	SIF TALENT:ARG:눈 == 7 && RAND:DICE == 0
		LOCALS = 강직한 눈매를 가진 
	
	SIF TALENT:ARG:차밍포인트 == 10 && RAND:DICE == 0
		LOCALS = 깨끗한 손가락인
	SIF TALENT:ARG:차밍포인트 == 13 && RAND:DICE == 0
		LOCALS = 잘록함이 자랑인 
	SIF TALENT:ARG:차밍포인트 == 14 && RAND:DICE == 0
		LOCALS = 엉덩이 형태가 예쁜
	SIF TALENT:ARG:차밍포인트 == 15 && RAND:DICE == 0
		LOCALS = 늘씬한 다리를 지닌
	SIF TALENT:ARG:차밍포인트 == 22 && RAND:DICE == 0
		LOCALS = 요염한 머리결을 지닌 
	SIF TALENT:ARG:차밍포인트 == 23 && RAND:DICE == 0
		LOCALS = 커다란 엉덩이를 가진 
	SIF TALENT:ARG:차밍포인트 == 25 && RAND:DICE == 0
		LOCALS = 덧니가 사랑스러운 
	
	SIF TALENT:ARG:용자가되기이전의생활 == 8 && RAND:DICE == 0
		LOCALS = 전귀족으로
	SIF TALENT:ARG:용자가되기이전의생활 == 12 && RAND:DICE == 0
		LOCALS = 원래 성녀로
	;SIF TALENT:ARG:204 && RAND:DICE == 0
	;	LOCALS = 육변기로 
	SIF TALENT:ARG:99 && RAND:DICE == 0
		LOCALS = 키가 큰 
	SIF TALENT:ARG:100 && RAND:DICE == 0
		LOCALS = 작은 
	SIF TALENT:ARG:256 && RAND:DICE == 0
		LOCALS = 안색이 나쁜 
	SIF (TALENT:ARG:21 || TALENT:ARG:22) && RAND:DICE == 0
		LOCALS = 새침한 얼굴로 
	SIF TALENT:ARG:35 && RAND:DICE == 0
		LOCALS = 수줍은 얼굴의
	SIF (TALENT:ARG:15 || TALENT:ARG:16) && RAND:DICE == 0
		LOCALS = 버릇없는 여자의 
	SIF TALENT:ARG:17 && RAND:DICE == 0
		LOCALS = 비굴하게 웃는 얼굴의 
	SIF TALENT:ARG:12 && RAND:DICE == 0
		LOCALS = 겁없는 웃는 얼굴의 
	SIF (TALENT:ARG:10 || TALENT:ARG:26) && RAND:DICE == 0
		LOCALS = 울것만같은 
	SIF (TALENT:ARG:23 || TALENT:ARG:25) && RAND:DICE == 0
		LOCALS = 건강이 가득한 
	SIF TALENT:ARG:73 && RAND:DICE == 0
		LOCALS = 엉덩이가 가벼운 
	
	SIF CFLAG:ARG:509 == 1 && RAND:DICE == 0
		LOCALS = 미아의 
	SIF INRANGE(ABL:ARG:37, 1, 3) && RAND:DICE == 0
		LOCALS = 몸을 파는 기쁨을 알아버린 
	;ABL系は共存しうるため変化を出すためにとりあえず確率で
	SIF ABL:ARG:30 == 10 && RAND:DICE == 0
		LOCALS = 자지없는 생활은 생각할수조차 없게 되어버린
	SIF ABL:ARG:31 == 10 && RAND:DICE == 0
		LOCALS = 한가 할때면 자위만 하고있는
	SIF ABL:ARG:32 == 10 && RAND:DICE == 0
		LOCALS = 냄새나는 정액을 아주 좋아하게 된
	SIF ABL:ARG:33 == 10 && RAND:DICE == 0
		LOCALS = 여성을 보면 머리속으로 범하지 않고는 배길수 없게된
	SIF ABL:ARG:37 == 10 && RAND:DICE == 0
		LOCALS = 언제부터인가 매춘의 포로가 되어버린

	;陥落済みである
	IF TALENT:ARG:76
		LOCALS += "엣찌한 "
	ELSEIF TALENT:ARG:85
		LOCALS += "배신자의 "
	ENDIF
	LOCALS += @"%GET_LOOK_INFO(ARG, "종족")%의 %SAVESTR:ARG%"
CASEELSE
	THROW 불명한 TYPE%TYPE%
ENDSELECT
RETURNF LOCALS



;売春ログ表示文字列関数
;プレイ回数や客の合計を出力
@FS_LOG_BITCH(TYPE, ARG:1, ARG:2, ARG:3, ARG:4, ARG:5)
#FUNCTIONS
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS TYPE
#DIM LCOUNT
VARSET LOCALS
SELECTCASE TYPE
CASE "DUNGEON_MAN", "DUNGEON_GIRL", "TOWN_MAN", "TOWN_GIRL"
	FOR LCOUNT, 1, 6
		SIF !ARG:LCOUNT
			CONTINUE
		SIF STRLENS(LOCALS)
			LOCALS += ", "
		LOCALS += @"{ARG:LCOUNT}사람의 %FS_BITCH(TYPE, LCOUNT)%"
	NEXT
CASE "PLAYNAME"
	FOR LCOUNT, 1, 6
		SIF !ARG:LCOUNT
			CONTINUE
		SIF STRLENS(LOCALS)
			LOCALS += ", "
		LOCALS += @"{ARG:LCOUNT}회의 %FS_BITCH("PLAYNAME", LCOUNT)%"
	NEXT
CASEELSE
	THROW 불명한 TYPE%TYPE%
ENDSELECT
RETURNF LOCALS



;売春描写関数呼び出し関数
;売春が全て終わって（成功して）から呼ぶ
;BIT利用で売春の内容を記録した引数CHECKを元に描写分岐を行う
;0 　　　=TOWN/DUNGEON
;1～6　　プレイ内容（1=HAND, 2=ORAL, 3=LES, 4=ANAL, 5=SEX, 6=ANIMAL）
;11～15　男性客の種類
;21～25　女性客の種類
;ARG＝キャラ登録番号はこの関数では使用しないが描写関数側で必要となるためそのまま渡す
@LOG_AFTER_BITCH(ARG, CHECK)
#LOCALSIZE 1
#LOCALSSIZE 1
#DIM CHECK
#DIM KYAKU
#DIM PLAY
;けも
IF GETBIT(CHECK, 6)
	PLAY = 6
;れず
ELSEIF GETBIT(CHECK, 3) && RAND:10 < 5
	DO
		KYAKU = RAND(1, 6)
		SIF GETBIT(CHECK, (KYAKU + 20))
			BREAK
	LOOP 1
	PLAY = 3
;ふつう
ELSE
	;デバッグ加筆
	;恐らくレズ設定でRAND判定から外れてこっちに来た時に
	;女性客を取りたいのに男性客から判定しているため
	;無限ループが発生していると解釈して
	;処置を行いました
	IF GETBIT(CHECK, 3)
		DO
			KYAKU = RAND(1, 6)
			SIF GETBIT(CHECK, (KYAKU + 20))
				BREAK
		LOOP 1
		PLAY = 3
	ELSE
		DO
			KYAKU = RAND(1, 6)
			SIF GETBIT(CHECK, (KYAKU + 10))
				BREAK
		LOOP 1
		DO
			PLAY = RAND(1, 6)
			SIF PLAY == 3
				CONTINUE
			SIF GETBIT(CHECK, PLAY)
				BREAK
		LOOP 1
	ENDIF
	
ENDIF

LOCALS = %FS_BITCH("PLAY", PLAY)%
IF GETBIT(CHECK, 0)
	CALLFORM LOG_BITCH_%LOCALS%(ARG, "DUNGEON", KYAKU)
ELSE
	CALLFORM LOG_BITCH_%LOCALS%(ARG, "TOWN", KYAKU)
ENDIF
WAIT



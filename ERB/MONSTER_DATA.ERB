@MONSTER_SETUP(ARG)
#DIM EXTRA
#DIM SELECT
#DIM FLOOR
#DIM ROOM
;----------------------------------

;エラー弾き
SIF ARG == 999
	RETURN 999
SIF ARG < 100
	RETURN 999
SIF ARG > 199
	RETURN 999

IF ARG < 190
	FLOOR = ((ARG - 100) / 10) + 1
	LOCAL = FLOOR + 349
	ROOM = FLAG:LOCAL
ELSE
	FLOOR = 0
	ROOM = 0
ENDIF

$START

LOCAL = ARG + 700
EXTRA = FLAG:LOCAL

CALL MONSTER_NAME,ARG,0
PRINTL 의 개조
PRINTFORML 소지금　{MONEY}
IF ROOM >= 500 && ROOM < 510
	PRINTFORML 시설：%ITEMNAME:ROOM%
ELSE
	PRINTL 시설：통로
ENDIF

LOCAL:1 = 0
LOCAL:2 = EXTRA
LOCAL:2 %= 100
FOR LOCAL,0,10
	CALL MONSTER_SETUP_ABLE,ARG,LOCAL
	IF LOCAL == 0
		PRINT [0] 무개조
	ELSEIF LOCAL == 1 && RESULT == 1
		PRINT [1] 상급화
	ELSEIF LOCAL == 2 && RESULT == 1
		PRINT [2] 토지적응
	ELSEIF LOCAL == 3 && RESULT == 1
		PRINT [3] 산성화
	ELSEIF LOCAL == 4 && RESULT == 1
		PRINT [4] 맹독화
	ELSEIF LOCAL == 5 && RESULT == 1
		PRINT [5] 장갑화
	ENDIF
	
	IF LOCAL:2 == LOCAL
		PRINT * 
	ELSE
		PRINT  
	ENDIF
	
	LOCAL:1 += 1
	LOCAL:1 %= 6
	SIF LOCAL:1 == 0
		PRINTL  
	
NEXT

SIF LOCAL:1 != 0
	PRINTL  

;魔族・여자마족・아인・요정・巨人は병종も選択できる

CALL MONSTER_DATA,ARG,0

IF E:7 == 1 || E:7 == 6 || E:7 == 7 || E:7 == 8 || E:7 == 9
	PRINTL --- 병종 ---

	LOCAL:1 = 0
	LOCAL:2 = EXTRA
	LOCAL:2 /= 100
	LOCAL:2 %= 100
	LOCAL:2 += 50
	FOR LOCAL,50,60
		CALL MONSTER_SETUP_ABLE,ARG,LOCAL
		IF LOCAL == 50
			PRINT [50] 통상병
		ELSEIF LOCAL == 51 && RESULT == 1
			PRINT [51] 궁병
		ELSEIF LOCAL == 52 && RESULT == 1
			PRINT [52] 마도병
		ELSEIF LOCAL == 53 && RESULT == 1
			PRINT [53] 최면사
		ELSEIF LOCAL == 54 && RESULT == 1
			PRINT [54] 고기갑옷병
		ENDIF
		
		IF LOCAL:2 == LOCAL
			PRINT * 
		ELSE
			PRINT  
		ENDIF
		
		LOCAL:1 += 1
		LOCAL:1 %= 6
		SIF LOCAL:1 == 0
			PRINTL  
		
	NEXT
	
	SIF LOCAL:1 != 0
		PRINTL  
	
ENDIF

DRAWLINE
PRINTL [999] 돌아간다

INPUT

SIF RESULT == 999
	RETURN 999

SELECT = RESULT

CALL MONSTER_SETUP_ABLE,ARG,SELECT

SIF RESULT == 0
	RETURN 999

PRINTL 설정변경에 1000포인트가 소모됩니다.
IF MONEY < 1000
	PRINTW 포인트가 부족합니다
	RETURN 999
ENDIF

CALL SELECT_YES_NO
SIF RESULT == 1
	RETURN 999

;該当区域を入れ替える
IF SELECT < 50
	LOCAL = EXTRA
	LOCAL /= 100
	LOCAL *= 100
	SELECT += LOCAL
	EXTRA = SELECT
ELSEIF SELECT >= 50 && SELECT < 100
	SELECT -= 50
	SELECT *= 100
	LOCAL = EXTRA
	LOCAL %= 100
	SELECT += LOCAL
	LOCAL = EXTRA
	LOCAL /= 10000
	LOCAL *= 10000
	SELECT += LOCAL
	EXTRA = SELECT
ENDIF

LOCAL = ARG + 700
FLAG:LOCAL = EXTRA
MONEY -= 1000

GOTO START

;----------------------------------
@MONSTER_SETUP_ABLE(ARG,ARG:1)
;----------------------------------
;ARG = モンスターID
;ARG:1 = メニュー

;無印化は誰でも可
SIF ARG:1 == 0
	RETURN 1

;上級は誰でも可
SIF ARG:1 == 1
	RETURN 1

;토지적응はID190未満なら可
SIF ARG:1 == 2 && ARG < 190
	RETURN 1

CALL MONSTER_DATA,ARG,0

;산성
;スライムか触手のみ
SIF (E:7 == 2 || E:7 == 5) && ARG:1 == 3
	RETURN 1

;장갑
;スライム以外
SIF E:7 != 2 && ARG:1 == 5
	RETURN 1

IF E:7 == 1 || E:7 == 6 || E:7 == 7 || E:7 == 8 || E:7 == 9
	;병종
	;통상병・궁병・肉鎧兵は誰でも選択可
	SIF ARG:1 == 50 || ARG:1 == 51 || ARG:1 == 54
		RETURN 1
	;エナジーボルトを知らない場合、마도병選択可
	SIF E:6 != 3 && ARG:1 == 52
		RETURN 1
	;スリープを知らない場合、최면사選択可
	SIF E:6 != 2 && ARG:1 == 53
		RETURN 1
	
ELSE
	;人間以外
	
	;맹독
	SIF ARG:1 == 4
		RETURN 1
	
ENDIF

RETURN 0


;ITEM:野良犬がないとダメ
SIF ITEM:22 == 0
	RETURN 0
;瀕死だとダメ
SIF BASE:0 < 500
	RETURN 0

B = 0

;수간중독
IF ABL:39 == 0
	B -= 2
ELSEIF ABL:39 == 1
	B -= 1
ELSEIF ABL:39 == 2
	B += 0
ELSEIF ABL:39 == 3
	B += 1
ELSEIF ABL:39 == 4
	B += 2
ELSEIF ABL:39 == 5
	B += 3
ELSEIF ABL:39 >= 6
	B += 4
ENDIF

;動物耳で欲望ＬＶ３以上PALAM:欲情がLV3以上
SIF TALENT:124 && ABL:11 >= 3 && PALAM:5 >= PALAMLV:3
	B += 1

;PALAM:欲情がLV4以上
;欲望ＬＶ５以上露出癖４以上で+1（下と合わせて+2）
SIF ABL:11 >= 5 && ABL:17 >= 4 && PALAM:5 >= PALAMLV:4
	B += 1
;欲望ＬＶ４以上露出癖３以上で+1
SIF ABL:11 >= 4 && ABL:17 >= 3 && PALAM:5 >= PALAMLV:4
	B += 1

;牝犬によるボーナス
SIF TALENT:136
	B += 2

;回数が0以下なら終了
SIF B <= 0
	RETURN 0

;프라이드낮음、해방、動物耳によるボーナス
SIF TALENT:17
	B += 1
SIF TALENT:33
	B += 1
SIF TALENT:124
	B += 1

;프라이드높음、자제심、억압、汚臭敏感によるペナルティ
SIF TALENT:15
	B -= 1
SIF TALENT:20
	B -= 1
SIF TALENT:32
	B -= 1
SIF TALENT:62 && TALENT:64 == 0
	B -= 2

;쾌감에솔직、쾌감을부정
IF TALENT:70
	B += 1
ELSEIF TALENT:71
	B -= 2
ENDIF

;淫乱によるボーナス
SIF TALENT:76
	B += 1

;牝犬によるボーナスその２
SIF TALENT:136
	TIMES B , 1.50

;回数が0以下になっていたら終了
SIF B <= 0
	RETURN 0

DRAWLINE
PRINTFORM 그 후、%조사선택(SAVESTR:TARGET, "는")% 떠돌이 개를 기르고 있는 축사에 숨어들어、
PRINTFORML {B}회의 교미를 한 것 같다。

;수간경험
PRINTFORML %EXPNAME:56%＋{B}
PRINTFORML %EXPNAME:0%＋{B}
PRINTFORML %EXPNAME:5%＋{B}
EXP:56 += B
EXP:0 += B
EXP:5 += B

;珠経験
PRINTFORML %PALAMNAME:1%의 구슬＋{B*200}
PRINTFORML %PALAMNAME:6%의 구슬＋{B*300}
PRINTFORMW %PALAMNAME:8%의 구슬＋{B*200}
JUEL:1 += A*200
JUEL:6 += A*300
JUEL:8 += A*200

;순종+露出+マゾっ気が12以上かつ時間が朝なら主人に報告
IF ABL:10+ABL:17+ABL:21 >= 12 && TIME == 0
	PRINTFORM 그것을 %조사선택(SAVESTR:TARGET, "는")% 
	SIF TALENT:124
		PRINT 꼬리를 흔들면서 
	PRINTFORML 보고해왔다。
	PRINTFORML %PALAMNAME:8%의 구슬＋{B*200}
	JUEL:8 += A*200
ENDIF



RETURN 1

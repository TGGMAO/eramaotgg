S = BASE:PLAYER:2
EJAC = MAXBASE:PLAYER:2

IF  S > EJAC * 2
	E = 2
ELSEIF S > EJAC
	E = 1
ELSE
	E = 0
ENDIF

IF E
;사정している
	TIMES SOURCE:4 , 3.00

	;ABL:精液中毒をみる
	IF ABL:32 == 0
		SOURCE:7 = 0
		TIMES SOURCE:5 , 2.00
		TIMES SOURCE:13, 2.00
	ELSEIF ABL:32 == 1
		SOURCE:7 = 50
		TIMES SOURCE:5 , 2.50
		TIMES SOURCE:13, 1.60
	ELSEIF ABL:32 == 2
		SOURCE:7 = 150
		TIMES SOURCE:5 , 3.00
		TIMES SOURCE:13, 1.00
	ELSEIF ABL:32 == 3
		SOURCE:7 = 300
		TIMES SOURCE:5 , 4.50
		TIMES SOURCE:13, 0.70
	ELSEIF ABL:32 == 4
		SOURCE:7 = 600
		TIMES SOURCE:5 , 6.00
		TIMES SOURCE:13, 0.40
	ELSE
		SOURCE:7 = 1200
		TIMES SOURCE:5 , 8.00
		TIMES SOURCE:13, 0.10
	ENDIF
ENDIF

;대량사정
IF E == 2
	TIMES SOURCE:7 , 2.00
	TIMES SOURCE:5 , 1.50

	EXP:PLAYER:3 += 2
	EXP:20 += 1
	PRINTFORML %SAVESTR:PLAYER%대량사정
	PRINTL 정액경험＋１
	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:2 -= EJAC*2
	SIF BASE:PLAYER:2 >= EJAC
		BASE:PLAYER:2 = EJAC-1

	;조수が사정フラグ
	TFLAG:6 = 2
;通常の사정
ELSEIF E == 1
	EXP:PLAYER:3 += 1
	PRINTFORML %SAVESTR:PLAYER%사정
	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:2 -= EJAC
	SIF BASE:PLAYER:2 >= EJAC
		BASE:PLAYER:2 = EJAC-1

	;조수が사정フラグ
	TFLAG:6 = 1
ENDIF

IF TALENT:119 || TALENT:121 || TALENT:122
	;노예のＰ⇔調教者のＶの汚れが移動
	STAIN:2 |= STAIN:PLAYER:3
	STAIN:PLAYER:3 |= STAIN:2
ENDIF

IF TALENT:122 == 0
	PRINTS EXPNAME:40
	PRINTL +10
	EXP:40 += 10
ENDIF

TFLAG:100 = 1

;屈服刻印２に相当
TFLAG:200 = 2

RETURN 1


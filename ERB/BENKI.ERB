	IF ABL:(ARG):13 >= 2 && RAND:DICE == 0
		ANSWER = 31
		DICE += 1
	ENDIF
	
	;露出癖2以上で自慰分岐
	IF ABL:(ARG):17 >= 2 && RAND:DICE == 0
		ANSWER = 3
		DICE += 1
	ENDIF
	
	;奉仕精神2以上でアナル奉仕分岐
	IF ABL:(ARG):16 >= 2 && RAND:DICE == 0
		ANSWER = 37
		DICE += 1
	ENDIF
	
	;V感覚2以上で正常位分岐
	CALL V_ABLE,ARG
	IF ABL:(ARG):2 >= 2 && RESULT == 1 && RAND:DICE == 0
		ANSWER = 20
		DICE += 1
	ENDIF
	
	;V感覚2以上で後背位分岐
	CALL V_ABLE,ARG
	IF ABL:(ARG):2 >= 2 && RESULT == 1 && RAND:DICE == 0
		ANSWER = 21
		DICE += 1
	ENDIF
	
	;A感覚2以上で正常位アナル分岐
	IF ABL:(ARG):3 >= 2 && RAND:DICE == 0
		ANSWER = 26
		DICE += 1
	ENDIF
	
	;A感覚2以上で後背位アナル分岐
	IF ABL:(ARG):3 >= 2 && RAND:DICE == 0
		ANSWER = 27
		DICE += 1
	ENDIF
	
ENDIF

RETURN ANSWER


		GOTJUEL:JUEL_COUNT = GET_JUEL + EX:4 * 1000
	ELSEIF JUEL_COUNT < 11 && JUEL_COUNT != 14 && JUEL_COUNT != 15
		GOTJUEL:JUEL_COUNT = GET_JUEL
	ELSE
		GOTJUEL:100 += GET_JUEL

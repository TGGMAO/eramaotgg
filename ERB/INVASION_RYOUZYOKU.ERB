;----------------------------------
@INVASION_RYOUZYOKU, ARG:0, ARG:1
;----------------------------------
#DIM ID
#DIM NUM
#DIM RYOUZYOKU

;侵攻ポイントを調整
;基本的に0∼10の範囲に抑える
ARG:1 /= 5000

;戦争で陵辱
REPEAT 3
	X = (RAND:9 + 1) * 10 + 100 + RAND:5
	CALL MONSTER_DATA, X, COUNT, 0, -1

	RYOUZYOKU = COUNT * 100 + 7
	NUM = COUNT * 100 + 99
	IF E:NUM <= 0
		E:RYOUZYOKU = 0
	ENDIF
	IF E:RYOUZYOKU > 0
		ID = E:(COUNT*100)
		PRINTFORMW %ITEMNAME:ID%의 능욕이 시작되었다
	ENDIF
	IF E:RYOUZYOKU == 1
		;カタコト
		CALL ORC_INV, ARG:0, ARG:1
	ELSEIF E:RYOUZYOKU == 2
		;슬라임
		CALL SLIME_INV, ARG:0, ARG:1
	ELSEIF E:RYOUZYOKU == 3
		;곤충
		CALL INSECT_INV, ARG:0, ARG:1
	ELSEIF E:RYOUZYOKU == 4
		;蔦촉수
		CALL IVY_INV, ARG:0, ARG:1
	ELSEIF E:RYOUZYOKU == 5
		;촉수
		CALL SYOKUSYU_INV, ARG:0, ARG:1
	ELSEIF E:RYOUZYOKU == 6
		;요정
		CALL FAILY_INV, ARG:0, ARG:1
	ELSEIF E:RYOUZYOKU == 7
		;거인
		CALL GIANT_INV, ARG:0, ARG:1
	ELSEIF E:RYOUZYOKU == 8
		;男
		CALL MAN_INV, ARG:0, ARG:1
	ELSEIF E:RYOUZYOKU == 9
		;女
		CALL GIRL_INV, ARG:0, ARG:1
	ELSEIF E:RYOUZYOKU == 10
		;야수
		CALL BEAST_INV, ARG:0, ARG:1
	ELSEIF E:RYOUZYOKU == 11
		;뇌간
		CALL BRAIN_INV, ARG:0, ARG:1
	ELSEIF E:RYOUZYOKU == 12
		;말
		CALL HORSE_INV, ARG:0, ARG:1
	ENDIF
	PRINTL  
REND

RETURN 0



